package swethlab;

public class Exp2
{
  public static void main(String[] args)
  {
	if(args.length==0)
	{
			System.out.println("No arguments provided");
	}
	else 
	{
	 int Sum = 0;
			for(String num :args)
			{
				Sum +=Integer.parseInt(num);
			}
			System.out.println("Sum of the provided arguments:"+Sum);
			
		}
	}
}

